{
  "name": "react-media-components",
  "version": "0.0.1",
  "description": "Components in react for recording and playing back videos.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npx cypress run",
    "test:unit": "npx cypress run -- --spec 'cypress/components/*.spec.tsx'",
    "build": "./node_modules/.bin/babel src --presets=@babel/preset-env,@babel/preset-react,@babel/preset-typescript --out-dir dist --extensions='.ts,.tsx'",
    "prepare": "husky install",
    "cypress:run": "cypress run --headless --browser chrome",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gibsonjareds/react-media-components.git"
  },
  "keywords": [
    "webrtc",
    "video",
    "recording",
    "media"
  ],
  "author": "Jared Gibson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gibsonjareds/react-media-components/issues"
  },
  "homepage": "https://github.com/gibsonjareds/react-media-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/react": "^5.8.0",
    "@cypress/webpack-dev-server": "^1.3.1",
    "@cypress/webpack-preprocessor": "^5.8.0",
    "@types/react": "^17.0.8",
    "babel-loader": "^8.2.2",
    "cypress": "^7.3.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "semantic-ui-react": "^2.0.3",
    "ts-loader": "^8.3.0",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
